<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.zyp.mapper.UserMapper">
	<insert id="insertDictionarie" parameterType="dictionarie">
		<selectKey resultType="String" keyProperty="id" order="BEFORE">
			select replace(uuid(),'-','') from dual
		</selectKey>
		INSERT into `dictionarie`(id,parentId,name)
		VALUES(#{id},#{parentId},#{name})
	</insert>
	<select id="dictionarieList" parameterType="com.zyp.util.Pagination" resultType="dictionarie">
		SELECT * FROM `dictionarie` u
		limit #{skipNum},#{takeNum}
	</select>
	<select id="dictionariecount" parameterType="com.zyp.util.Pagination" resultType="int">
		SELECT count(1) FROM `dictionarie` u
		limit #{skipNum},#{takeNum}
	</select>
</mapper>